<UserControl x:Class="DXApplication2.SelectionAPersonalDetails"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:controls="clr-namespace:Microsoft.Toolkit.Wpf.UI.Controls;assembly=Microsoft.Toolkit.Wpf.UI.Controls"
      xmlns:editors="http://schemas.devexpress.com/winfx/2008/xaml/editors"
             xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core"
             mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800" Loaded="UserControl_Loaded"
      >
    <Grid>
        
  
      <Grid>
            <Grid.RowDefinitions>
                <RowDefinition Height="2*"></RowDefinition>
                <RowDefinition Height="1*"></RowDefinition>
            </Grid.RowDefinitions>
            <Grid Grid.Row="0" Margin="20" HorizontalAlignment="Stretch">
                <ScrollViewer>
                    <Grid>
                        <Grid.RowDefinitions>
                            <RowDefinition></RowDefinition>
                            <RowDefinition></RowDefinition>
                            <RowDefinition></RowDefinition>
                            <RowDefinition></RowDefinition>
                            <RowDefinition></RowDefinition>
                            <RowDefinition></RowDefinition>
                            <RowDefinition></RowDefinition>
                        </Grid.RowDefinitions>

                        <!--start - Personal Details-->
                        <Grid Grid.Row="0">
                            <Grid.Resources>
                                <SolidColorBrush x:Key="brush" Color="Silver"/>
                            </Grid.Resources>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="1*"></ColumnDefinition>
                                <ColumnDefinition Width="2*"></ColumnDefinition>
                                <ColumnDefinition Width="2*"></ColumnDefinition>
                            </Grid.ColumnDefinitions>
                            <Grid.RowDefinitions>
                                <RowDefinition></RowDefinition>
                                <RowDefinition></RowDefinition>
                                <RowDefinition></RowDefinition>
                                <RowDefinition></RowDefinition>
                                <RowDefinition></RowDefinition>
                                <RowDefinition></RowDefinition>
                                <RowDefinition></RowDefinition>
                                <RowDefinition></RowDefinition>
                                <RowDefinition></RowDefinition>
                                <RowDefinition></RowDefinition>
                                <RowDefinition></RowDefinition>
                                <RowDefinition></RowDefinition>
                                <RowDefinition></RowDefinition>
                                <RowDefinition></RowDefinition>
                                <RowDefinition></RowDefinition>
                                <RowDefinition></RowDefinition>
                                <RowDefinition></RowDefinition>
                                <RowDefinition></RowDefinition>
                                <RowDefinition></RowDefinition>
                                <RowDefinition></RowDefinition>
                                <RowDefinition></RowDefinition>
                                <RowDefinition></RowDefinition>
                            </Grid.RowDefinitions>

                            <!--Grid Title-->
                            <Border Grid.Column="0" Grid.Row="0" BorderBrush="{StaticResource brush}" BorderThickness="1 1 0 0" Background="Gray" Padding="6" VerticalAlignment="Center">
                                <TextBlock Padding="3" Foreground="White" FontSize="16">Personal Details</TextBlock>
                            </Border>
                            <Border Grid.Column="1" Grid.Row="0" BorderBrush="{StaticResource brush}" BorderThickness="0 1 0 0" Background="Gray" Padding="6">

                            </Border>
                            <Border Grid.Column="2" Grid.Row="0" BorderBrush="{StaticResource brush}" BorderThickness="0 1 1 0" Background="Gray" Padding="6">

                            </Border>

                            <!--Grid Header-->
                            <Border Grid.Column="0" Grid.Row="1" BorderBrush="{StaticResource brush}" BorderThickness="1 1 0 0" Background="Silver" Padding="6">

                            </Border>
                            <Border Grid.Column="1" Grid.Row="1" BorderBrush="{StaticResource brush}" BorderThickness="1 1 0 0" Background="Silver" Padding="6" VerticalAlignment="Center">
                                <TextBlock Padding="3" FontWeight="Bold">Client 1</TextBlock>
                            </Border>
                            <Border Grid.Column="2" Grid.Row="1" BorderBrush="{StaticResource brush}" BorderThickness="1 1 1 0" Background="Silver" Padding="6" VerticalAlignment="Center">
                                <TextBlock Padding="3" FontWeight="Bold">Client 2</TextBlock>
                            </Border>

                            <!--Surname & Maiden Name-->
                            <Border Grid.Column="0" Grid.Row="2"  BorderBrush="{StaticResource brush}" BorderThickness="1 1 0 0" Background="WhiteSmoke">
                                <TextBlock TextAlignment="Right" Padding="3" TextWrapping="Wrap" VerticalAlignment="Center">Surname &amp; Maiden Name</TextBlock>
                            </Border>
                            <Border Grid.Column="1" Grid.Row="2"  BorderBrush="{StaticResource brush}" BorderThickness="1 1 0 0">
                                <editors:TextEdit x:Name="txtSurnameMaidenName_Client1" />
                            </Border>
                            <Border Grid.Column="2" Grid.Row="2"  BorderBrush="{StaticResource brush}" BorderThickness="1 1 1 0">
                                <editors:TextEdit x:Name="txtSurnameMaidenName_Client2" />
                            </Border>

                            <!--Given name/s-->
                            <Border Grid.Column="0" Grid.Row="3"  BorderBrush="{StaticResource brush}" BorderThickness="1 1 0 0" Background="WhiteSmoke">
                                <TextBlock TextAlignment="Right" Padding="3" TextWrapping="Wrap" VerticalAlignment="Center">Given name/s</TextBlock>
                            </Border>
                            <Border Grid.Column="1" Grid.Row="3"  BorderBrush="{StaticResource brush}" BorderThickness="1 1 0 0">
                                <editors:TextEdit x:Name="txtGivenNames_Client1" />
                            </Border>
                            <Border Grid.Column="2" Grid.Row="3"  BorderBrush="{StaticResource brush}" BorderThickness="1 1 1 0">
                                <editors:TextEdit x:Name="txtGivenNames_Client2" />
                            </Border>

                            <!--Preferred (short) name-->
                            <Border Grid.Column="0" Grid.Row="4"  BorderBrush="{StaticResource brush}" BorderThickness="1 1 0 0" Background="WhiteSmoke">
                                <TextBlock TextAlignment="Right" Padding="3" TextWrapping="Wrap" VerticalAlignment="Center">Preferred (short) name</TextBlock>
                            </Border>
                            <Border Grid.Column="1" Grid.Row="4"  BorderBrush="{StaticResource brush}" BorderThickness="1 1 0 0">
                                <editors:TextEdit x:Name="txtPreferredShortName_Client1" />
                            </Border>
                            <Border Grid.Column="2" Grid.Row="4"  BorderBrush="{StaticResource brush}" BorderThickness="1 1 1 0">
                                <editors:TextEdit x:Name="txtPreferredShortName_Client2" />
                            </Border>

                            <!--Title (Mr, Mrs, Ms, Sir, Dr)-->
                            <Border Grid.Column="0" Grid.Row="5"  BorderBrush="{StaticResource brush}" BorderThickness="1 1 0 0" Background="WhiteSmoke">
                                <TextBlock TextAlignment="Right" Padding="3" TextWrapping="Wrap" VerticalAlignment="Center">Title (Mr, Mrs, Ms, Sir, Dr)</TextBlock>
                            </Border>
                            <Border Grid.Column="1" Grid.Row="5"  BorderBrush="{StaticResource brush}" BorderThickness="1 1 0 0">
                                <editors:TextEdit x:Name="txtTitle_Client1" />
                            </Border>
                            <Border Grid.Column="2" Grid.Row="5"  BorderBrush="{StaticResource brush}" BorderThickness="1 1 1 0">
                                <editors:TextEdit x:Name="txtTitle_Client2" />
                            </Border>

                            <!--Gender-->
                            <Border Grid.Column="0" Grid.Row="6"  BorderBrush="{StaticResource brush}" BorderThickness="1 1 0 0" Background="WhiteSmoke">
                                <TextBlock TextAlignment="Right" Padding="3" TextWrapping="Wrap" VerticalAlignment="Center">Gender</TextBlock>
                            </Border>
                            <Border Grid.Column="1" Grid.Row="6"  BorderBrush="{StaticResource brush}" BorderThickness="1 1 0 0">
                                <Grid>
                                    <Grid.RowDefinitions>
                                        <RowDefinition/>
                                    </Grid.RowDefinitions>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="Auto"/>
                                        <ColumnDefinition Width="Auto"/>
                                    </Grid.ColumnDefinitions>
                                    <RadioButton x:Name="rbGenderMale_Client1" Content="Male" GroupName="Gender_Client1" Grid.Row="1" Grid.Column="0" />
                                    <RadioButton x:Name="rbGenderFemale_Client1" Content="Female" GroupName="Gender_Client1"  Grid.Row="1" Grid.Column="1" />
                                </Grid>
                            </Border>
                            <Border Grid.Column="2" Grid.Row="6"  BorderBrush="{StaticResource brush}" BorderThickness="1 1 1 0">
                                <Grid>
                                    <Grid.RowDefinitions>
                                        <RowDefinition/>
                                    </Grid.RowDefinitions>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="Auto"/>
                                        <ColumnDefinition Width="Auto"/>
                                    </Grid.ColumnDefinitions>
                                    <RadioButton x:Name="rbGenderMale_Client2" Content="Male" GroupName="Gender_Client2" Grid.Row="0" Grid.Column="0" />
                                    <RadioButton x:Name="rbGenderFemale_Client2" Content="Female" GroupName="Gender_Client2" Grid.Row="0" Grid.Column="1" />
                                </Grid>
                            </Border>

                            <!--Date of birth-->
                            <Border Grid.Column="0" Grid.Row="7"  BorderBrush="{StaticResource brush}" BorderThickness="1 1 0 0" Background="WhiteSmoke">
                                <TextBlock TextAlignment="Right" Padding="3" TextWrapping="Wrap" VerticalAlignment="Center">Date of birth</TextBlock>
                            </Border>
                            <Border Grid.Column="1" Grid.Row="7"  BorderBrush="{StaticResource brush}" BorderThickness="1 1 0 0">
                                <editors:DateEdit x:Name="dtDateOfBirth_Client1" DisplayFormatString="dd/MM/yyyy">
                                  
                                </editors:DateEdit>
                            </Border>
                            <Border Grid.Column="2" Grid.Row="7"  BorderBrush="{StaticResource brush}" BorderThickness="1 1 1 0">
                                <editors:DateEdit x:Name="dtDateOfBirth_Client2" DisplayFormatString="dd/MM/yyyy">
                                
                                </editors:DateEdit>
                            </Border>

                            <!--Age-->
                            <Border Grid.Column="0" Grid.Row="8"  BorderBrush="{StaticResource brush}" BorderThickness="1 1 0 0" Background="WhiteSmoke">
                                <TextBlock TextAlignment="Right" Padding="3" TextWrapping="Wrap" VerticalAlignment="Center">Age</TextBlock>
                            </Border>
                            <Border Grid.Column="1" Grid.Row="8"  BorderBrush="{StaticResource brush}" BorderThickness="1 1 0 0">
                                <editors:SpinEdit x:Name="spnAge_Client1" DisplayFormatString="C" MinValue="0" MaxValue="99">
                                    
                                </editors:SpinEdit>
                            </Border>
                            <Border Grid.Column="2" Grid.Row="8"  BorderBrush="{StaticResource brush}" BorderThickness="1 1 1 0">
                                <editors:SpinEdit x:Name="spnAge_Client2" DisplayFormatString="C" MinValue="0" MaxValue="99">
                                   
                                </editors:SpinEdit>
                            </Border>

                            <!--Marital status-->
                            <Border Grid.Column="0" Grid.Row="9"  BorderBrush="{StaticResource brush}" BorderThickness="1 1 0 0" Background="WhiteSmoke">
                                <TextBlock TextAlignment="Right" Padding="3" TextWrapping="Wrap" VerticalAlignment="Center">Marital status</TextBlock>
                            </Border>
                            <Border Grid.Column="1" Grid.Row="9"  BorderBrush="{StaticResource brush}" BorderThickness="1 1 0 0">
                                <Grid>
                                    <Grid.RowDefinitions>
                                        <RowDefinition/>
                                        <RowDefinition/>
                                        <RowDefinition/>
                                    </Grid.RowDefinitions>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="Auto"/>
                                        <ColumnDefinition Width="Auto"/>
                                    </Grid.ColumnDefinitions>
                                    <RadioButton x:Name="rbMarried_Client1" Content="Married" GroupName="MaritalStatus_Client1" Grid.Row="0" Grid.Column="0" />
                                    <RadioButton x:Name="rbDivorced_Client1" Content="Divorced" GroupName="MaritalStatus_Client1"  Grid.Row="0" Grid.Column="1" />
                                    <RadioButton x:Name="rbSingle_Client1" Content="Single" GroupName="MaritalStatus_Client1" Grid.Row="1" Grid.Column="0" />
                                    <RadioButton x:Name="rbWidowEd1_Client1" Content="Widow(ed)" GroupName="MaritalStatus_Client1" Grid.Row="1" Grid.Column="1" />
                                    <RadioButton x:Name="rbDeFacto_Client1" Content="De facto" GroupName="MaritalStatus_Client1" Grid.Row="2" Grid.Column="0" />
                                    <RadioButton x:Name="rbUnknown_Client1" Content="Unknown" GroupName="MaritalStatus_Client1"  Grid.Row="2" Grid.Column="1" />
                                </Grid>
                            </Border>
                            <Border Grid.Column="2" Grid.Row="9"  BorderBrush="{StaticResource brush}" BorderThickness="1 1 1 0">
                                <Grid>
                                    <Grid.RowDefinitions>
                                        <RowDefinition/>
                                        <RowDefinition/>
                                        <RowDefinition/>
                                    </Grid.RowDefinitions>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="Auto"/>
                                        <ColumnDefinition Width="Auto"/>
                                    </Grid.ColumnDefinitions>
                                    <RadioButton x:Name="rbMarried_Client2" Content="Married" GroupName="MaritalStatus_Client2" Grid.Row="0" Grid.Column="0" />
                                    <RadioButton x:Name="rbDivorced_Client2" Content="Divorced" GroupName="MaritalStatus_Client2"  Grid.Row="0" Grid.Column="1" />
                                    <RadioButton x:Name="rbSingle_Client2" Content="Single" GroupName="MaritalStatus_Client2" Grid.Row="1" Grid.Column="0" />
                                    <RadioButton x:Name="rbWidowEd1_Client2" Content="Widow(ed)" GroupName="MaritalStatus_Client2" Grid.Row="1" Grid.Column="1" />
                                    <RadioButton x:Name="rbDeFacto_Client2" Content="De facto" GroupName="MaritalStatus_Client2" Grid.Row="2" Grid.Column="0" />
                                    <RadioButton x:Name="rbUnknown_Client2" Content="Unknown" GroupName="MaritalStatus_Client2"  Grid.Row="2" Grid.Column="1" />
                                </Grid>
                            </Border>

                            <!--Health-->
                            <Border Grid.Column="0" Grid.Row="10"  BorderBrush="{StaticResource brush}" BorderThickness="1 1 0 0" Background="WhiteSmoke">
                                <TextBlock TextAlignment="Right" Padding="3" TextWrapping="Wrap" VerticalAlignment="Center">Health</TextBlock>
                            </Border>
                            <Border Grid.Column="1" Grid.Row="10"  BorderBrush="{StaticResource brush}" BorderThickness="1 1 0 0">
                                <Grid>
                                    <Grid.RowDefinitions>
                                        <RowDefinition/>
                                        <RowDefinition/>
                                    </Grid.RowDefinitions>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="Auto"/>
                                        <ColumnDefinition Width="Auto"/>
                                    </Grid.ColumnDefinitions>
                                    <RadioButton x:Name="rbExcellent_Client1" Content="Excellent" GroupName="Health_Client1" Grid.Row="0" Grid.Column="0" />
                                    <RadioButton x:Name="rbPoor_Client1" Content="Poor" GroupName="Health_Client1"  Grid.Row="0" Grid.Column="1" />
                                    <RadioButton x:Name="rbGood_Client1" Content="Good" GroupName="Health_Client1" Grid.Row="1" Grid.Column="0" />
                                    <RadioButton x:Name="rbUnknown_HealthClient1" Content="Unknown" GroupName="Health_Client1" Grid.Row="1" Grid.Column="1" />

                                </Grid>
                            </Border>
                            <Border Grid.Column="2" Grid.Row="10"  BorderBrush="{StaticResource brush}" BorderThickness="1 1 1 0">
                                <Grid>
                                    <Grid.RowDefinitions>
                                        <RowDefinition/>
                                        <RowDefinition/>
                                        <RowDefinition/>
                                    </Grid.RowDefinitions>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="Auto"/>
                                        <ColumnDefinition Width="Auto"/>
                                    </Grid.ColumnDefinitions>
                                    <RadioButton x:Name="rbExcellent_Client2" Content="Excellent" GroupName="Health_Client2" Grid.Row="0" Grid.Column="0" />
                                    <RadioButton x:Name="rbPoor_Client2" Content="Poor" GroupName="Health_Client2"  Grid.Row="0" Grid.Column="1" />
                                    <RadioButton x:Name="rbGood_Client2" Content="Good" GroupName="Health_Client2" Grid.Row="1" Grid.Column="0" />
                                    <RadioButton x:Name="rbUnknown_HealthClient2" Content="Unknown" GroupName="Health_Client2" Grid.Row="1" Grid.Column="1" />

                                </Grid>
                            </Border>

                            <!--Smoker-->
                            <Border Grid.Column="0" Grid.Row="11"  BorderBrush="{StaticResource brush}" BorderThickness="1 1 0 0" Background="WhiteSmoke">
                                <TextBlock TextAlignment="Right" Padding="3" TextWrapping="Wrap" VerticalAlignment="Center">Smoker</TextBlock>
                            </Border>
                            <Border Grid.Column="1" Grid.Row="11"  BorderBrush="{StaticResource brush}" BorderThickness="1 1 0 0">
                                <Grid>
                                    <Grid.RowDefinitions>
                                        <RowDefinition/>

                                    </Grid.RowDefinitions>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="Auto"/>
                                        <ColumnDefinition Width="Auto"/>
                                    </Grid.ColumnDefinitions>
                                    <RadioButton x:Name="rbYes_SmokerClient1" Content="Yes" GroupName="Smoker_Client1" Grid.Row="0" Grid.Column="0" />
                                    <RadioButton x:Name="rbNo_SmokerClient1" Content="No" GroupName="Smoker_Client1"  Grid.Row="0" Grid.Column="1" />


                                </Grid>
                            </Border>
                            <Border Grid.Column="2" Grid.Row="11"  BorderBrush="{StaticResource brush}" BorderThickness="1 1 1 0">
                                <Grid>
                                    <Grid.RowDefinitions>
                                        <RowDefinition/>
                                        <RowDefinition/>
                                        <RowDefinition/>
                                    </Grid.RowDefinitions>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="Auto"/>
                                        <ColumnDefinition Width="Auto"/>
                                    </Grid.ColumnDefinitions>
                                    <RadioButton x:Name="rbYes_SmokerClient2" Content="Yes" GroupName="Smoker_Client2" Grid.Row="0" Grid.Column="0" />
                                    <RadioButton x:Name="rbNo_SmokerClient2" Content="No" GroupName="Smoker_Client2"  Grid.Row="0" Grid.Column="1" />

                                </Grid>
                            </Border>

                            <!--Town of birth-->
                            <Border Grid.Column="0" Grid.Row="12"  BorderBrush="{StaticResource brush}" BorderThickness="1 1 0 0" Background="WhiteSmoke">
                                <TextBlock TextAlignment="Right" Padding="3" TextWrapping="Wrap" VerticalAlignment="Center">Town of birth</TextBlock>
                            </Border>
                            <Border Grid.Column="1" Grid.Row="12"  BorderBrush="{StaticResource brush}" BorderThickness="1 1 0 0">
                                <editors:TextEdit x:Name="txtTownOfBirth_Client1" />
                            </Border>
                            <Border Grid.Column="2" Grid.Row="12"  BorderBrush="{StaticResource brush}" BorderThickness="1 1 1 0">
                                <editors:TextEdit x:Name="txtTownOfBirth_Client2" />
                            </Border>

                            <!--Country of birth-->
                            <Border Grid.Column="0" Grid.Row="13"  BorderBrush="{StaticResource brush}" BorderThickness="1 1 0 0" Background="WhiteSmoke">
                                <TextBlock TextAlignment="Right" Padding="3" TextWrapping="Wrap" VerticalAlignment="Center">Country of birth</TextBlock>
                            </Border>
                            <Border Grid.Column="1" Grid.Row="13"  BorderBrush="{StaticResource brush}" BorderThickness="1 1 0 0">
                                <editors:TextEdit x:Name="txtCountryOfBirth_Client1" />
                            </Border>
                            <Border Grid.Column="2" Grid.Row="13"  BorderBrush="{StaticResource brush}" BorderThickness="1 1 1 0">
                                <editors:TextEdit x:Name="txtCountryOfBirth_Client2" />
                            </Border>

                            <!--If not Australia – years in Australia-->
                            <Border Grid.Column="0" Grid.Row="14"  BorderBrush="{StaticResource brush}" BorderThickness="1 1 0 0" Background="WhiteSmoke">
                                <TextBlock TextAlignment="Right" Padding="3" TextWrapping="Wrap" VerticalAlignment="Center">If not Australia – years in Australia</TextBlock>
                            </Border>
                            <Border Grid.Column="1" Grid.Row="14"  BorderBrush="{StaticResource brush}" BorderThickness="1 1 0 0">
                                <editors:TextEdit x:Name="txtIfnotAustraliayearsinAustralia_Client1" />
                            </Border>
                            <Border Grid.Column="2" Grid.Row="14"  BorderBrush="{StaticResource brush}" BorderThickness="1 1 1 0">
                                <editors:TextEdit x:Name="txtIfnotAustraliayearsinAustralia_Client2" />
                            </Border>

                            <!--Are you a non-resident?-->
                            <Border Grid.Column="0" Grid.Row="15"  BorderBrush="{StaticResource brush}" BorderThickness="1 1 0 0" Background="WhiteSmoke">
                                <TextBlock TextAlignment="Right" Padding="3" TextWrapping="Wrap" VerticalAlignment="Center">Are you a non-resident?</TextBlock>
                            </Border>
                            <Border Grid.Column="1" Grid.Row="15"  BorderBrush="{StaticResource brush}" BorderThickness="1 1 0 0">
                                <editors:TextEdit x:Name="txtAreyouanonresident_Client1" />
                            </Border>
                            <Border Grid.Column="2" Grid.Row="15"  BorderBrush="{StaticResource brush}" BorderThickness="1 1 1 0">
                                <editors:TextEdit x:Name="txtAreyouanonresident_Client2" />
                            </Border>

                            <!--Tax file number-->
                            <Border Grid.Column="0" Grid.Row="16"  BorderBrush="{StaticResource brush}" BorderThickness="1 1 0 0" Background="WhiteSmoke">
                                <TextBlock TextAlignment="Right" Padding="3" TextWrapping="Wrap" VerticalAlignment="Center">Tax file number</TextBlock>
                            </Border>
                            <Border Grid.Column="1" Grid.Row="16"  BorderBrush="{StaticResource brush}" BorderThickness="1 1 0 0">
                                <editors:TextEdit x:Name="txtTaxfilenumber_Client1" />
                            </Border>
                            <Border Grid.Column="2" Grid.Row="16"  BorderBrush="{StaticResource brush}" BorderThickness="1 1 1 0">
                                <editors:TextEdit x:Name="txtTaxfilenumber_Client2" />
                            </Border>

                            <!--Were you referred to us?-->
                            <Border Grid.Column="0" Grid.Row="17"  BorderBrush="{StaticResource brush}" BorderThickness="1 1 0 0" Background="WhiteSmoke">
                                <TextBlock TextAlignment="Right" Padding="3" TextWrapping="Wrap" VerticalAlignment="Center">Were you referred to us?</TextBlock>
                            </Border>
                            <Border Grid.Column="1" Grid.Row="17"  BorderBrush="{StaticResource brush}" BorderThickness="1 1 0 0">
                                <editors:TextEdit x:Name="txtWereyoureferredtous_Client1" />
                            </Border>
                            <Border Grid.Column="2" Grid.Row="17"  BorderBrush="{StaticResource brush}" BorderThickness="1 1 1 0">
                                <editors:TextEdit x:Name="txtWereyoureferredtous_Client2" />
                            </Border>

                            <!--Have you worked with another adviser?-->
                            <Border Grid.Column="0" Grid.Row="18"  BorderBrush="{StaticResource brush}" BorderThickness="1 1 0 0" Background="WhiteSmoke">
                                <TextBlock TextAlignment="Right" Padding="3" TextWrapping="Wrap" VerticalAlignment="Center">Have you worked with another adviser?</TextBlock>
                            </Border>
                            <Border Grid.Column="1" Grid.Row="18"  BorderBrush="{StaticResource brush}" BorderThickness="1 1 0 0">
                                <editors:TextEdit x:Name="txtHaveyouworkedwithanotheradviser_Client1" />
                            </Border>
                            <Border Grid.Column="2" Grid.Row="18"  BorderBrush="{StaticResource brush}" BorderThickness="1 1 1 0">
                                <editors:TextEdit x:Name="txtHaveyouworkedwithanotheradviser_Client2" />
                            </Border>

                            <!--Are you a resident of another country for tax purposes?-->
                            <Border Grid.Column="0" Grid.Row="19"  BorderBrush="{StaticResource brush}" BorderThickness="1 1 0 0" Background="WhiteSmoke">
                                <TextBlock TextAlignment="Right" Padding="3" TextWrapping="Wrap" VerticalAlignment="Center">Are you a resident of another country for tax purposes?</TextBlock>
                            </Border>
                            <Border Grid.Column="1" Grid.Row="19"  BorderBrush="{StaticResource brush}" BorderThickness="1 1 0 0">
                                <editors:TextEdit x:Name="txtAreyouaresidentofanothercountryfortaxpurposes_Client1" />
                            </Border>
                            <Border Grid.Column="2" Grid.Row="19"  BorderBrush="{StaticResource brush}" BorderThickness="1 1 1 0">
                                <editors:TextEdit x:Name="txtAreyouaresidentofanothercountryfortaxpurposes_Client2" />
                            </Border>

                            <!--If yes, country of residence-->
                            <Border Grid.Column="0" Grid.Row="20"  BorderBrush="{StaticResource brush}" BorderThickness="1 1 0 0" Background="WhiteSmoke">
                                <TextBlock TextAlignment="Right" Padding="3" TextWrapping="Wrap" VerticalAlignment="Center">If yes, country of residence</TextBlock>
                            </Border>
                            <Border Grid.Column="1" Grid.Row="20"  BorderBrush="{StaticResource brush}" BorderThickness="1 1 0 0">
                                <editors:TextEdit x:Name="txtIfyescountryofresidence_Client1" />
                            </Border>
                            <Border Grid.Column="2" Grid.Row="20"  BorderBrush="{StaticResource brush}" BorderThickness="1 1 1 0">
                                <editors:TextEdit x:Name="txtIfyescountryofresidence_Client2" />
                            </Border>

                            <!--Tax identification number (TIN) of foreign country-->
                            <Border Grid.Column="0" Grid.Row="21"  BorderBrush="{StaticResource brush}" BorderThickness="1 1 0 0" Background="WhiteSmoke">
                                <TextBlock TextAlignment="Right" Padding="3" TextWrapping="Wrap" VerticalAlignment="Center">Tax identification number (TIN) of foreign country</TextBlock>
                            </Border>
                            <Border Grid.Column="1" Grid.Row="21"  BorderBrush="{StaticResource brush}" BorderThickness="1 1 0 0">
                                <editors:TextEdit x:Name="txtTaxidentificationnumberTINofforeigncountry_Client1" />
                            </Border>
                            <Border Grid.Column="2" Grid.Row="21"  BorderBrush="{StaticResource brush}" BorderThickness="1 1 1 0">
                                <editors:TextEdit x:Name="txtTaxidentificationnumberTINofforeigncountry_Client2" />
                            </Border>

                            <!--Are you a politically exposed person?-->
                            <Border Grid.Column="0" Grid.Row="22"  BorderBrush="{StaticResource brush}" BorderThickness="1 1 0 1" Background="WhiteSmoke">
                                <TextBlock TextAlignment="Right" Padding="3" TextWrapping="Wrap" VerticalAlignment="Center">Are you a politically exposed person?</TextBlock>
                            </Border>
                            <Border Grid.Column="1" Grid.Row="22"  BorderBrush="{StaticResource brush}" BorderThickness="1 1 0 1">
                                <editors:TextEdit x:Name="txtAreyouapoliticallyexposedperson_Client1" />
                            </Border>
                            <Border Grid.Column="2" Grid.Row="22"  BorderBrush="{StaticResource brush}" BorderThickness="1 1 1 1">
                                <editors:TextEdit x:Name="txtAreyouapoliticallyexposedperson_Client2" />
                            </Border>

                        </Grid>
                        <!--end - Personal Details-->

                        <!--start - Contact Details -->
                        <Grid Grid.Row="1" Margin="0 20 0 0">
                            <Grid.Resources>
                                <SolidColorBrush x:Key="brush" Color="Silver"/>
                            </Grid.Resources>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="1*"></ColumnDefinition>
                                <ColumnDefinition Width="2*"></ColumnDefinition>
                                <ColumnDefinition Width="2*"></ColumnDefinition>
                            </Grid.ColumnDefinitions>
                            <Grid.RowDefinitions>
