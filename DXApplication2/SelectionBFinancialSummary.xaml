<UserControl x:Class="DXApplication2.SelectionBFinancialSummary"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:DXApplication2"
             xmlns:my="http://schemas.devexpress.com/winfx/2008/xaml/editors"
             xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="2*"></RowDefinition>
            <RowDefinition Height="1*"></RowDefinition>
        </Grid.RowDefinitions>
        <Grid Grid.Row="0" Margin="20" HorizontalAlignment="Stretch">
            <ScrollViewer>
                <Grid>
                    <Grid.RowDefinitions>
                        <RowDefinition></RowDefinition>
                        <RowDefinition></RowDefinition>
                        <RowDefinition></RowDefinition>
                        <RowDefinition></RowDefinition>
                        <RowDefinition></RowDefinition>
                        <RowDefinition></RowDefinition>
                        <RowDefinition></RowDefinition>
                        <RowDefinition></RowDefinition>
                    </Grid.RowDefinitions>

                    <!--start - Employment Details-->
                    <Grid Grid.Row="0">
                        <Grid.Resources>
                            <SolidColorBrush x:Key="brush" Color="Silver"/>
                        </Grid.Resources>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="1*"></ColumnDefinition>
                            <ColumnDefinition Width="2*"></ColumnDefinition>
                            <ColumnDefinition Width="2*"></ColumnDefinition>
                        </Grid.ColumnDefinitions>
                        <Grid.RowDefinitions>
                            <RowDefinition></RowDefinition>
                            <RowDefinition></RowDefinition>
                            <RowDefinition></RowDefinition>
                            <RowDefinition></RowDefinition>
                            <RowDefinition></RowDefinition>
                            <RowDefinition></RowDefinition>
                            <RowDefinition></RowDefinition>
                            <RowDefinition></RowDefinition>
                            <RowDefinition></RowDefinition>

                        </Grid.RowDefinitions>

                        <!--Grid Title-->
                        <Border Grid.Column="0" Grid.Row="0" BorderBrush="{StaticResource brush}" BorderThickness="1 1 0 0" Background="Gray" Padding="6" VerticalAlignment="Center">
                            <TextBlock Padding="3" Foreground="White" FontSize="16">Employment Details</TextBlock>
                        </Border>
                        <Border Grid.Column="1" Grid.Row="0" BorderBrush="{StaticResource brush}" BorderThickness="0 1 0 0" Background="Gray" Padding="6">

                        </Border>
                        <Border Grid.Column="2" Grid.Row="0" BorderBrush="{StaticResource brush}" BorderThickness="0 1 1 0" Background="Gray" Padding="6">

                        </Border>

                        <!--Grid Header-->
                        <Border Grid.Column="0" Grid.Row="1" BorderBrush="{StaticResource brush}" BorderThickness="1 1 0 0" Background="Silver" Padding="6">

                        </Border>
                        <Border Grid.Column="1" Grid.Row="1" BorderBrush="{StaticResource brush}" BorderThickness="1 1 0 0" Background="Silver" Padding="6" VerticalAlignment="Center">
                            <TextBlock Padding="3" FontWeight="Bold">Client 1</TextBlock>
                        </Border>
                        <Border Grid.Column="2" Grid.Row="1" BorderBrush="{StaticResource brush}" BorderThickness="1 1 1 0" Background="Silver" Padding="6" VerticalAlignment="Center">
                            <TextBlock Padding="3" FontWeight="Bold">Client 2</TextBlock>
                        </Border>


                        <!--Work status-->
                        <Border Grid.Column="0" Grid.Row="2"  BorderBrush="{StaticResource brush}" BorderThickness="1 1 0 0" Background="WhiteSmoke">
                            <TextBlock TextAlignment="Right" Padding="3"  TextWrapping="Wrap" VerticalAlignment="Center">Work status</TextBlock>
                        </Border>
                        <Border Grid.Column="1" Grid.Row="2"  BorderBrush="{StaticResource brush}" BorderThickness="1 1 0 0">
                            <Grid>
                                <Grid.RowDefinitions>
                                    <RowDefinition />
                                    <RowDefinition />
                                    <RowDefinition />
                                    <RowDefinition />
                                    <RowDefinition />
                                    <RowDefinition />
                                    <RowDefinition />
                                    <RowDefinition />
                                    <RowDefinition />
                                </Grid.RowDefinitions>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="Auto"/>
                                </Grid.ColumnDefinitions>
                                <RadioButton x:Name="rbFullyemployed_Client1" Content="Fully employed" GroupName="Workstatus_Client1" Grid.Row="0" Grid.Column="0" />
                                <RadioButton x:Name="rbParttimeemployed_Client1" Content="Part time employed" GroupName="Workstatus_Client1" Grid.Row="1" Grid.Column="0" />
                                <RadioButton x:Name="rbHomeduties_Client1" Content="Home duties" GroupName="Workstatus_Client1" Grid.Row="2" Grid.Column="0" />
                                <RadioButton x:Name="rbSelfemployed_Client1" Content="Self employed" GroupName="Workstatus_Client1" Grid.Row="3" Grid.Column="0" />
                                <RadioButton x:Name="rbUnemployed_Client1" Content="Unemployed" GroupName="Workstatus_Client1" Grid.Row="4" Grid.Column="0" />
                                <RadioButton x:Name="rbEarlyRetirement_Client1" Content="Early Retirement" GroupName="Workstatus_Client1" Grid.Row="5" Grid.Column="0" />
                                <RadioButton x:Name="rbRetired_Client1" Content="Retired" GroupName="Workstatus_Client1" Grid.Row="6" Grid.Column="0" />
                                <RadioButton x:Name="rbStudent_Client1" Content="Student" GroupName="Workstatus_Client1" Grid.Row="7" Grid.Column="0" />
                                <RadioButton x:Name="rbUnknown_Client1" Content="Unknown" GroupName="Workstatus_Client1" Grid.Row="8" Grid.Column="0" />

                            </Grid>
                        </Border>
                        <Border Grid.Column="2" Grid.Row="2"  BorderBrush="{StaticResource brush}" BorderThickness="1 1 1 0">
                            <Grid>
                                <Grid.RowDefinitions>
                                    <RowDefinition />
                                    <RowDefinition />
                                    <RowDefinition />
                                    <RowDefinition />
                                    <RowDefinition />
                                    <RowDefinition />
                                    <RowDefinition />
                                    <RowDefinition />
                                    <RowDefinition />
                                </Grid.RowDefinitions>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="Auto"/>

                                </Grid.ColumnDefinitions>
                                <RadioButton x:Name="rbFullyemployed_Client2" Content="Fully employed" GroupName="Workstatus_Client2" Grid.Row="0" Grid.Column="0" />
                                <RadioButton x:Name="rbParttimeemployed_Client2" Content="Part time employed" GroupName="Workstatus_Client2" Grid.Row="1" Grid.Column="0" />
                                <RadioButton x:Name="rbHomeduties_Client2" Content="Home duties" GroupName="Workstatus_Client2" Grid.Row="2" Grid.Column="0" />
                                <RadioButton x:Name="rbSelfemployed_Client2" Content="Self employed" GroupName="Workstatus_Client2" Grid.Row="3" Grid.Column="0" />
                                <RadioButton x:Name="rbUnemployed_Client2" Content="Unemployed" GroupName="Workstatus_Client2" Grid.Row="4" Grid.Column="0" />
                                <RadioButton x:Name="rbEarlyRetirement_Client2" Content="Early Retirement" GroupName="Workstatus_Client2" Grid.Row="5" Grid.Column="0" />
                                <RadioButton x:Name="rbRetired_Client2" Content="Retired" GroupName="Workstatus_Client2" Grid.Row="6" Grid.Column="0" />
                                <RadioButton x:Name="rbStudent_Client2" Content="Student" GroupName="Workstatus_Client2" Grid.Row="7" Grid.Column="0" />
                                <RadioButton x:Name="rbUnknown_Client2" Content="Unknown" GroupName="Workstatus_Client2" Grid.Row="8" Grid.Column="0" />

                            </Grid>
                        </Border>

                        <!--Employer-->
                        <Border Grid.Column="0" Grid.Row="3"  BorderBrush="{StaticResource brush}" BorderThickness="1 1 0 0" Background="WhiteSmoke">
                            <TextBlock TextAlignment="Right" Padding="3" TextWrapping="Wrap" VerticalAlignment="Center">Employer</TextBlock>
                        </Border>
                        <Border Grid.Column="1" Grid.Row="3"  BorderBrush="{StaticResource brush}" BorderThickness="1 1 0 0">
                            <my:TextEdit x:Name="txtEmployer_Client1" />
                        </Border>
                        <Border Grid.Column="2" Grid.Row="3"  BorderBrush="{StaticResource brush}" BorderThickness="1 1 1 0">
                            <my:TextEdit x:Name="txtEmployer_Client2" />
                        </Border>

                        <!--Employer address-->
                        <Border Grid.Column="0" Grid.Row="4"  BorderBrush="{StaticResource brush}" BorderThickness="1 1 0 0" Background="WhiteSmoke">
                            <TextBlock TextAlignment="Right" Padding="3" TextWrapping="Wrap" VerticalAlignment="Center">Employer address</TextBlock>
                        </Border>
                        <Border Grid.Column="1" Grid.Row="4"  BorderBrush="{StaticResource brush}" BorderThickness="1 1 0 0">
                            <my:TextEdit x:Name="txtEmployeraddress_Client1" />
                        </Border>
                        <Border Grid.Column="2" Grid.Row="4"  BorderBrush="{StaticResource brush}" BorderThickness="1 1 1 0">
                            <my:TextEdit x:Name="txtEmployeraddress_Client2" />
                        </Border>

                        <!--Occupation-->
                        <Border Grid.Column="0" Grid.Row="5"  BorderBrush="{StaticResource brush}" BorderThickness="1 1 0 0" Background="WhiteSmoke">
                            <TextBlock TextAlignment="Right" Padding="3" TextWrapping="Wrap" VerticalAlignment="Center">Occupation</TextBlock>
                        </Border>
                        <Border Grid.Column="1" Grid.Row="5"  BorderBrush="{StaticResource brush}" BorderThickness="1 1 0 0">
                            <my:TextEdit x:Name="txtOccupation_Client1" />
                        </Border>
                        <Border Grid.Column="2" Grid.Row="5"  BorderBrush="{StaticResource brush}" BorderThickness="1 1 1 0">
                            <my:TextEdit x:Name="txtOccupation_Client2" />
                        </Border>

                        <!--Number of years’ service-->
                        <Border Grid.Column="0" Grid.Row="6"  BorderBrush="{StaticResource brush}" BorderThickness="1 1 0 0" Background="WhiteSmoke">
                            <TextBlock TextAlignment="Right" Padding="3" TextWrapping="Wrap" VerticalAlignment="Center">Number of years’ service</TextBlock>
                        </Border>
                        <Border Grid.Column="1" Grid.Row="6"  BorderBrush="{StaticResource brush}" BorderThickness="1 1 0 0">
                            <my:TextEdit x:Name="txtNumberofyearsservice_Client1" />
                        </Border>
                        <Border Grid.Column="2" Grid.Row="6"  BorderBrush="{StaticResource brush}" BorderThickness="1 1 1 0">
                            <my:TextEdit x:Name="txtNumberofyearsservice_Client2" />
                        </Border>

                        <!--(Intended) Retirement date-->
                        <Border Grid.Column="0" Grid.Row="7"  BorderBrush="{StaticResource brush}" BorderThickness="1 1 0 0" Background="WhiteSmoke">
                            <TextBlock TextAlignment="Right" Padding="3" TextWrapping="Wrap" VerticalAlignment="Center">(Intended) Retirement date</TextBlock>
                        </Border>
                        <Border Grid.Column="1" Grid.Row="7"  BorderBrush="{StaticResource brush}" BorderThickness="1 1 0 0">
                            <my:TextEdit x:Name="txtIntendedRetirementdate_Client1" />
                        </Border>
                        <Border Grid.Column="2" Grid.Row="7"  BorderBrush="{StaticResource brush}" BorderThickness="1 1 1 0">
                            <my:TextEdit x:Name="txtIntendedRetirementdate_Client2" />
                        </Border>

                        <!--Do you expect employment income to change within the next 12 months?-->
                        <Border Grid.Column="0" Grid.Row="8"  BorderBrush="{StaticResource brush}" BorderThickness="1 1 0 1" Background="WhiteSmoke">
                            <TextBlock TextAlignment="Right" Padding="3" TextWrapping="Wrap" VerticalAlignment="Center">Do you expect employment income to change within the next 12 months?</TextBlock>
                        </Border>
                        <Border Grid.Column="1" Grid.Row="8"  BorderBrush="{StaticResource brush}" BorderThickness="1 1 0 1">
                            <my:TextEdit x:Name="txtDoyouexpectemployment_Client1" />
                        </Border>
                        <Border Grid.Column="2" Grid.Row="8"  BorderBrush="{StaticResource brush}" BorderThickness="1 1 1 1">
                            <my:TextEdit x:Name="txtDoyouexpectemployment_Client2" />
                        </Border>

                    </Grid>
                    <!--end - Employment Details-->

                    <!--start - Salary & Other Income-->
                    <Grid Grid.Row="1" Margin="0 20 0 0">
                        <Grid.Resources>
                            <SolidColorBrush x:Key="brush" Color="Silver"/>
                            <SolidColorBrush x:Key="brushRed" Color="Red"/>
                        </Grid.Resources>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="1*"></ColumnDefinition>
                            <ColumnDefinition Width="2*"></ColumnDefinition>
                            <ColumnDefinition Width="2*"></ColumnDefinition>
                        </Grid.ColumnDefinitions>
                        <Grid.RowDefinitions>
                            <RowDefinition></RowDefinition>
                            <RowDefinition></RowDefinition>
                            <RowDefinition></RowDefinition>
                            <RowDefinition></RowDefinition>
                            <RowDefinition></RowDefinition>
                            <RowDefinition></RowDefinition>
                            <RowDefinition></RowDefinition>
                            <RowDefinition></RowDefinition>
                            <RowDefinition></RowDefinition>
                            <RowDefinition></RowDefinition>
                            <RowDefinition></RowDefinition>
                            <RowDefinition></RowDefinition>
                            <RowDefinition></RowDefinition>
                            <RowDefinition></RowDefinition>
                            <RowDefinition></RowDefinition>
                            <RowDefinition></RowDefinition>
                            <RowDefinition></RowDefinition>
                            <RowDefinition></RowDefinition>
                            <RowDefinition></RowDefinition>
                            <RowDefinition></RowDefinition>
                            <RowDefinition></RowDefinition>
                            <RowDefinition></RowDefinition>
                        </Grid.RowDefinitions>

                        <!--Grid Title-->
                        <Border Grid.Column="0" Grid.Row="0" BorderBrush="{StaticResource brush}" BorderThickness="1 1 0 0" Background="Gray" Padding="6" VerticalAlignment="Center">
                            <TextBlock Padding="3" Foreground="White" FontSize="16">Salary &amp; Other Income</TextBlock>
                        </Border>
                        <Border Grid.Column="1" Grid.Row="0" BorderBrush="{StaticResource brush}" BorderThickness="0 1 0 0" Background="Gray" Padding="6">

                        </Border>
                        <Border Grid.Column="2" Grid.Row="0" BorderBrush="{StaticResource brush}" BorderThickness="0 1 1 0" Background="Gray" Padding="6">

                        </Border>

                        <!--Grid Header-->
                        <Border Grid.Column="0" Grid.Row="1" BorderBrush="{StaticResource brush}" BorderThickness="1 1 0 0" Background="Silver" Padding="6">

                        </Border>
                        <Border Grid.Column="1" Grid.Row="1" BorderBrush="{StaticResource brush}" BorderThickness="1 1 0 0" Background="Silver" Padding="6" VerticalAlignment="Center">
                            <TextBlock Padding="3" FontWeight="Bold">Client 1</TextBlock>
                        </Border>
                        <Border Grid.Column="2" Grid.Row="1" BorderBrush="{StaticResource brush}" BorderThickness="1 1 1 0" Background="Silver" Padding="6" VerticalAlignment="Center">
                            <TextBlock Padding="3" FontWeight="Bold">Client 2</TextBlock>
                        </Border>

                        <!--Salary income-->
                        <Border Grid.Column="0" Grid.Row="3"  BorderBrush="{StaticResource brush}" BorderThickness="1 1 0 0" Background="WhiteSmoke">
                            <Grid>
                                <Grid.RowDefinitions>
                                    <RowDefinition></RowDefinition>
                                    <RowDefinition></RowDefinition>

                                </Grid.RowDefinitions>
                                <Grid>
                                    <TextBlock TextAlignment="Right" Padding="3" TextWrapping="Wrap" VerticalAlignment="Center">Salary income</TextBlock>

                                </Grid>
                                <Grid Grid.Row="1">
                                    <TextBlock TextAlignment="Right" Padding="3" TextWrapping="Wrap" VerticalAlignment="Center" FontSize="11" Foreground="Red">(please supply a recent payslip &amp; most current tax return)</TextBlock>

                                </Grid>
                            </Grid>

                        </Border>
                        <Border Grid.Column="1" Grid.Row="3"  BorderBrush="{StaticResource brush}" BorderThickness="1 1 0 0">
                            <my:TextEdit x:Name="txtSalaryincome_Client1" />
                        </Border>
                        <Border Grid.Column="2" Grid.Row="3"  BorderBrush="{StaticResource brush}" BorderThickness="1 1 1 0">
                            <my:TextEdit x:Name="txtSalaryincome_Client2" />
                        </Border>

                        <!--Other taxable income-->
                        <Border Grid.Column="0" Grid.Row="4"  BorderBrush="{StaticResource brush}" BorderThickness="1 1 0 0" Background="WhiteSmoke">
                            <TextBlock TextAlignment="Right" Padding="3" TextWrapping="Wrap" VerticalAlignment="Center">Other taxable income</TextBlock>
                        </Border>
                        <Border Grid.Column="1" Grid.Row="4"  BorderBrush="{StaticResource brush}" BorderThickness="1 1 0 0">
                            <my:TextEdit x:Name="txtOthertaxableincome_Client1" />
                        </Border>
                        <Border Grid.Column="2" Grid.Row="4"  BorderBrush="{StaticResource brush}" BorderThickness="1 1 1 0">
                            <my:TextEdit x:Name="txtOthertaxableincome_Client2" />
                        </Border>

                        <!--Tax free income-->
                        <Border Grid.Column="0" Grid.Row="5"  BorderBrush="{StaticResource brush}" BorderThickness="1 1 0 0" Background="WhiteSmoke">
                            <TextBlock TextAlignment="Right" Padding="3" TextWrapping="Wrap" VerticalAlignment="Center">Tax free income</TextBlock>
                        </Border>
                        <Border Grid.Column="1" Grid.Row="5"  BorderBrush="{StaticResource brush}" BorderThickness="1 1 0 0">
                            <my:TextEdit x:Name="txtTaxfreeincome_Client1" />
                        </Border>
                        <Border Grid.Column="2" Grid.Row="5"  BorderBrush="{StaticResource brush}" BorderThickness="1 1 1 0">
                            <my:TextEdit x:Name="txtTaxfreeincome_Client2" />
                        </Border>

                        <!--Family allowance-->
                        <Border Grid.Column="0" Grid.Row="6"  BorderBrush="{StaticResource brush}" BorderThickness="1 1 0 0" Background="WhiteSmoke">
                            <TextBlock TextAlignment="Right" Padding="3" TextWrapping="Wrap" VerticalAlignment="Center">Family allowance</TextBlock>
                        </Border>
                        <Border Grid.Column="1" Grid.Row="6"  BorderBrush="{StaticResource brush}" BorderThickness="1 1 0 0">
                            <my:TextEdit x:Name="txtFamilyallowance_Client1" />
                        </Border>
                        <Border Grid.Column="2" Grid.Row="6"  BorderBrush="{StaticResource brush}" BorderThickness="1 1 1 0">
                            <my:TextEdit x:Name="txtFamilyallowance_Client2" />
                        </Border>

                        <!--Directors fees / gratuities-->
                        <Border Grid.Column="0" Grid.Row="7"  BorderBrush="{StaticResource brush}" BorderThickness="1 1 0 0" Background="WhiteSmoke">
                            <TextBlock TextAlignment="Right" Padding="3" TextWrapping="Wrap" VerticalAlignment="Center">Directors fees / gratuities</TextBlock>
                        </Border>
                        <Border Grid.Column="1" Grid.Row="7"  BorderBrush="{StaticResource brush}" BorderThickness="1 1 0 0">
                            <my:TextEdit x:Name="txtDirectorsfeesgratuities_Client1" />
                        </Border>
                        <Border Grid.Column="2" Grid.Row="7"  BorderBrush="{StaticResource brush}" BorderThickness="1 1 1 0">
                            <my:TextEdit x:Name="txtDirectorsfeesgratuities_Client2" />
                        </Border>

                        <!--Are you expecting funds from estate or inheritance in the future?-->
                        <Border Grid.Column="0" Grid.Row="8"  BorderBrush="{StaticResource brush}" BorderThickness="1 1 0 0" Background="WhiteSmoke">
                            <TextBlock TextAlignment="Right" Padding="3" TextWrapping="Wrap" VerticalAlignment="Center"></TextBlock>
                        </Border>
                        <Border Grid.Column="1" Grid.Row="8"  BorderBrush="{StaticResource brush}" BorderThickness="1 1 0 0">
                            <my:TextEdit x:Name="txtAreyouexpectingfunds1_Client1" />
                        </Border>
                        <Border Grid.Column="2" Grid.Row="8"  BorderBrush="{StaticResource brush}" BorderThickness="1 1 1 0">
                            <my:TextEdit x:Name="txtAreyouexpectingfunds1_Client2" />
                        </Border>

                        <!--Are you expecting funds from estate or inheritance in the future? 2-->
                        <Border Grid.Column="0" Grid.Row="9"  BorderBrush="{StaticResource brush}" BorderThickness="1 0 0 0" Background="WhiteSmoke">
                            <TextBlock TextAlignment="Right" Padding="3" TextWrapping="Wrap" VerticalAlignment="Center">Are you expecting funds from estate or inheritance in the future?</TextBlock>
                        </Border>
                        <Border Grid.Column="1" Grid.Row="9"  BorderBrush="{StaticResource brush}" BorderThickness="1 1 0 0">
                            <my:TextEdit x:Name="txtAreyouexpectingfunds2_Client1" />
                        </Border>
                        <Border Grid.Column="2" Grid.Row="9"  BorderBrush="{StaticResource brush}" BorderThickness="1 1 1 0">
                            <my:TextEdit x:Name="txtAreyouexpectingfunds2_Client2" />
                        </Border>

                        <!--Are you expecting funds from estate or inheritance in the future? 3-->
                        <Border Grid.Column="0" Grid.Row="10"  BorderBrush="{StaticResource brush}" BorderThickness="1 0 0 0" Background="WhiteSmoke">
                            <TextBlock TextAlignment="Right" Padding="3" TextWrapping="Wrap" VerticalAlignment="Center"></TextBlock>
                        </Border>
                        <Border Grid.Column="1" Grid.Row="10"  BorderBrush="{StaticResource brush}" BorderThickness="1 1 0 0">
                            <my:TextEdit x:Name="txtAreyouexpectingfunds3_Client1" />
                        </Border>
                        <Border Grid.Column="2" Grid.Row="10"  BorderBrush="{StaticResource brush}" BorderThickness="1 1 1 0">
                            <my:TextEdit x:Name="txtAreyouexpectingfunds3_Client2" />
                        </Border>

                        <!--Other-->
                        <Border Grid.Column="0" Grid.Row="11"  BorderBrush="{StaticResource brush}" BorderThickness="1 1 0 0" Background="WhiteSmoke">
                            <TextBlock TextAlignment="Right" Padding="3" TextWrapping="Wrap" VerticalAlignment="Center"></TextBlock>
                        </Border>
                        <Border Grid.Column="1" Grid.Row="11"  BorderBrush="{StaticResource brush}" BorderThickness="1 1 0 0">
                            <my:TextEdit x:Name="txtOther1_Client1" />
                        </Border>
                        <Border Grid.Column="2" Grid.Row="11"  BorderBrush="{StaticResource brush}" BorderThickness="1 1 1 0">
                            <my:TextEdit x:Name="txtOther1_Client2" />
                        </Border>

                        <!--Other2-->
                        <Border Grid.Column="0" Grid.Row="12"  BorderBrush="{StaticResource brush}" BorderThickness="1 0 0 0" Background="WhiteSmoke">
                            <TextBlock TextAlignment="Right" Padding="3" TextWrapping="Wrap" VerticalAlignment="Center">Other</TextBlock>
                        </Border>
                        <Border Grid.Column="1" Grid.Row="12"  BorderBrush="{StaticResource brush}" BorderThickness="1 1 0 0">
                            <my:TextEdit x:Name="txtOther2_Client1" />
                        </Border>
                        <Border Grid.Column="2" Grid.Row="12"  BorderBrush="{StaticResource brush}" BorderThickness="1 1 1 0">
                            <my:TextEdit x:Name="txtOther2_Client2" />
                        </Border>

                        <!--Other3-->
                        <Border Grid.Column="0" Grid.Row="13"  BorderBrush="{StaticResource brush}" BorderThickness="1 0 0 1" Background="WhiteSmoke">
                            <TextBlock TextAlignment="Right" Padding="3" TextWrapping="Wrap" VerticalAlignment="Center"></TextBlock>
                        </Border>
                        <Border Grid.Column="1" Grid.Row="13"  BorderBrush="{StaticResource brush}" BorderThickness="1 1 0 1">
                            <my:TextEdit x:Name="txtOther3_Client1" />
                        </Border>
                        <Border Grid.Column="2" Grid.Row="13"  BorderBrush="{StaticResource brush}" BorderThickness="1 1 1 1">
                            <my:TextEdit x:Name="txtOther3_Client2" />
                        </Border>


                    </Grid>
                    <!--end - Salary & Other Income-->

                    <!--start - Salary Sacrifice / Package-->
                    <Grid Grid.Row="2" Margin="0 20 0 0">
                        <Grid.Resources>
                            <SolidColorBrush x:Key="brush" Color="Silver"/>
                            <SolidColorBrush x:Key="brushRed" Color="Red"/>
                        </Grid.Resources>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="1*"></ColumnDefinition>
                            <ColumnDefinition Width="2*"></ColumnDefinition>
                            <ColumnDefinition Width="2*"></ColumnDefinition>
                        </Grid.ColumnDefinitions>
                        <Grid.RowDefinitions>
                            <RowDefinition></RowDefinition>
                            <RowDefinition></RowDefinition>
                            <RowDefinition></RowDefinition>
                            <RowDefinition></RowDefinition>
                            <RowDefinition></RowDefinition>
                            <RowDefinition></RowDefinition>
                            <RowDefinition></RowDefinition>
                            <RowDefinition></RowDefinition>
                            <RowDefinition></RowDefinition>
                            <RowDefinition></RowDefinition>
                            <RowDefinition></RowDefinition>
                            <RowDefinition></RowDefinition>
                            <RowDefinition></RowDefinition>
                            <RowDefinition></RowDefinition>
                            <RowDefinition></RowDefinition>
                            <RowDefinition></RowDefinition>
                            <RowDefinition></RowDefinition>
                            <RowDefinition></RowDefinition>
                            <RowDefinition></RowDefinition>
                            <RowDefinition></RowDefinition>
                            <RowDefinition></RowDefinition>
                            <RowDefinition></RowDefinition>
                        </Grid.RowDefinitions>

                        <!--Grid Title-->
                        <Border Grid.Column="0" Grid.Row="0" BorderBrush="{StaticResource brush}" BorderThickness="1 1 0 0" Background="Gray" Padding="6" VerticalAlignment="Center">
                            <TextBlock Padding="3" Foreground="White" FontSize="16">Salary Sacrifice / Package</TextBlock>
                        </Border>
                        <Border Grid.Column="1" Grid.Row="0" BorderBrush="{StaticResource brush}" BorderThickness="0 1 0 0" Background="Gray" Padding="6">

                        </Border>
                        <Border Grid.Column="2" Grid.Row="0" BorderBrush="{StaticResource brush}" BorderThickness="0 1 1 0" Background="Gray" Padding="6">

                        </Border>

                        <!--Grid Header-->
                        <Border Grid.Column="0" Grid.Row="1" BorderBrush="{StaticResource brush}" BorderThickness="1 1 0 0" Background="Silver" Padding="6">

                        </Border>
                        <Border Grid.Column="1" Grid.Row="1" BorderBrush="{StaticResource brush}" BorderThickness="1 1 0 0" Background="Silver" Padding="6" VerticalAlignment="Center">
                            <TextBlock Padding="3" FontWeight="Bold">Client 1</TextBlock>
                        </Border>
                        <Border Grid.Column="2" Grid.Row="1" BorderBrush="{StaticResource brush}" BorderThickness="1 1 1 0" Background="Silver" Padding="6" VerticalAlignment="Center">
                            <TextBlock Padding="3" FontWeight="Bold">Client 2</TextBlock>
                        </Border>

                        <!--Salary income-->
                        <Border Grid.Column="0" Grid.Row="3"  BorderBrush="{StaticResource brush}" BorderThickness="1 1 0 0" Background="WhiteSmoke">
                            <Grid>
                                <Grid.RowDefinitions>
                                    <RowDefinition></RowDefinition>
                                    <RowDefinition></RowDefinition>

                                </Grid.RowDefinitions>
                                <Grid>
                                    <TextBlock TextAlignment="Right" Padding="3" TextWrapping="Wrap" VerticalAlignment="Center">Employment super contribution rate</TextBlock>

                                </Grid>
                                <Grid Grid.Row="1">
                                    <TextBlock TextAlignment="Right" Padding="3" TextWrapping="Wrap" VerticalAlignment="Center" FontSize="11" FontStyle="Italic">(e.g. 9.5%)</TextBlock>

                                </Grid>
                            </Grid>

                        </Border>
                        <Border Grid.Column="1" Grid.Row="3"  BorderBrush="{StaticResource brush}" BorderThickness="1 1 0 0">
                            <my:TextEdit x:Name="txtEmploymentsuper_Client1" />
                        </Border>
                        <Border Grid.Column="2" Grid.Row="3"  BorderBrush="{StaticResource brush}" BorderThickness="1 1 1 0">
                            <my:TextEdit x:Name="txtEmploymentsuper_Client2" />
